{
    "name": "Kupid",
    "version": "1.0",
    "manifest_version": 3,
    
    "content_scripts": [
        {
            "matches": ["https://www.okcupid.com/who-likes-you", "https://www.okcupid.com/discover"],
            "js": ["content.js"]
        }
    ],

    "permissions": [
        "storage"
    ],
    
    "icons": {
        "128": "resources/kupid.png"
    },

    "action": {
        "default_popup": "popup.html"
    }
}